# Offsets in x are 116.20 and 348.60 [ 1/2 and 3/2 * y offset ]
choffx1=-348.60
choffy1=232.40
choffx2=-116.20
choffy2=232.40
choffx3=116.20
choffy3=232.40
choffx4=348.60
choffy4=232.40
#
choffx5=-348.60
choffy5=-232.40
choffx6=-116.20
choffy6=-232.40
choffx7=116.20
choffy7=-232.40
choffx8=348.60
choffy8=-232.40
#
# dimx=8192
# dimy=8192
dimx=2048  # 1/4 8192
dimy=4096  # 1/2 8192
pixscale=0.1132   # Arcsec per pixel
